@prefix modgui: <http://moddevices.com/ns/modgui#> .
@prefix lv2:    <http://lv2plug.in/ns/lv2core#> .

<http://guitarix.sourceforge.net/plugins/gx_quack_#_quack_>
    modgui:gui [
        modgui:resourcesDirectory <modgui> ;
        modgui:iconTemplate <modgui/icon-gxquack.html> ;
        modgui:stylesheet <modgui/stylesheet-gxquack.css> ;
        modgui:screenshot <modgui/screenshot-gxquack.png> ;
        modgui:thumbnail <modgui/thumbnail-gxquack.png> ;
        modgui:brand "Guitarix " ;
        modgui:label "GxQuack" ;
        modgui:model "boxy" ;
        modgui:panel "4-knobs" ;
        modgui:color "quack" ;
        modgui:knob "cairo" ;
        modgui:port [
            lv2:index 0 ;
            lv2:symbol "GAIN" ;
            lv2:name "GAIN" ;
        ] , [
            lv2:index 1 ;
            lv2:symbol "DRIVE" ;
            lv2:name "DRIVE" ;
        ] , [
            lv2:index 2 ;
            lv2:symbol "TONE" ;
            lv2:name "TONE" ;
        ] , [
            lv2:index 3 ;
            lv2:symbol "PEAK" ;
            lv2:name "PEAK" ;
        ] , [
            lv2:index 4 ;
            lv2:symbol "DEPTH" ;
            lv2:name "DEPTH" ;
        ] , [
            lv2:index 5 ;
            lv2:symbol "RANGE" ;
            lv2:name "RANGE" ;
        ] , [
            lv2:index 6 ;
            lv2:symbol "MODE" ;
            lv2:name "MODE" ;
        ] ;
    ] .
